{
  "author": "Florian Keller <github@floriankeller.de>",
  "bin": {
    "gh-open": "dist/cli.js"
  },
  "dependencies": {
    "commander": "2.19.0",
    "find-up": "3.0.0",
    "opn": "5.5.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/find-up": "2.1.1",
    "@types/jasmine": "3.3.12",
    "@types/node": "11.11.5",
    "@types/opn": "5.1.0",
    "husky": "1.3.1",
    "jasmine": "3.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.4000"
  },
  "description": "Open a GitHub repository in your browser",
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "github",
    "repository-tools",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "spec/**/*.ts": [
      "tslint --config tslint.json --project tsconfig.jasmine.json --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/gh-open.js",
  "name": "@ffflorian/gh-open",
  "repository": "https://github.com/ffflorian/gh-open.git",
  "scripts": {
    "build": "tsc",
    "clear": "rimraf dist",
    "dist": "yarn clear && yarn build",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json \"**/*.ts\"",
    "prettier": "prettier --ignore-path .gitignore --write \"{src,spec}/*.ts\"",
    "release": "semantic-release",
    "test": "ts-node -P tsconfig.jasmine.json node_modules/jasmine/bin/jasmine"
  },
  "version": "1.1.3"
}
